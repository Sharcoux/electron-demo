{
  "name": "demo-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "extendable-media-recorder": "^6.1.53",
    "extendable-media-recorder-wav-encoder": "^7.0.47",
    "hazardous": "^0.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "vosk": "^0.3.24"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "preelectron": "NODE_ENV=development ./downloadLanguages.sh && npm run build",
    "prebuild-electron": "./downloadLanguages.sh && npm run build",
    "electron": "rm -rf dist && electron electron.js",
    "build-electron": "npm run build && rm -rf dist && electron-builder -wl",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "homepage": "./",
  "main": "public/electron.js",
  "devDependencies": {
    "electron": "^10.4.2",
    "electron-builder": "^22.10.5",
    "webpack-cli": "^4.6.0"
  },
  "build": {
    "extraResources": [
      "node_modules/at-least-node/**/*",
      "node_modules/builder-util-runtime/**/*",
      "node_modules/debug/**/*",
      "node_modules/ffi-napi/**/*",
      "node_modules/fs-extra/**/*",
      "node_modules/get-symbol-from-current-process-h/**/*",
      "node_modules/get-uv-event-loop-napi-h/**/*",
      "node_modules/graceful-fs/**/*",
      "node_modules/jsonfile/**/*",
      "node_modules/ms/**/*",
      "node_modules/node-addon-api/**/*",
      "node_modules/node-gyp-build/**/*",
      "node_modules/ref-napi/**/*",
      "node_modules/ref-struct-di/**/*",
      "node_modules/sax/**/*",
      "node_modules/universalify/**/*",
      "models/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}